<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoggedInController">
   <children>
      <Label fx:id="lable_welcome" layoutX="156.0" layoutY="28.0" text="Welcome!" textFill="#6a99f7">
         <font>
            <Font name="Comic Sans MS" size="64.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Label>
      <Button fx:id="button_logout" layoutX="180.0" layoutY="312.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="239.0" style="-fx-background-color: #8328CC;" text="Logout" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS" size="17.0" />
         </font>
      </Button>
      <Button fx:id="button_wordle" layoutX="108.0" layoutY="217.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="144.0" style="-fx-background-color: #8328CC;" text="Wordle" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="24.0" />
         </font>
      </Button>
      <Button fx:id="button_flags" layoutX="347.0" layoutY="217.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="144.0" style="-fx-background-color: #8328CC;" text="Flags" textAlignment="CENTER" textFill="WHITE">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="24.0" />
         </font>
      </Button>
      <ImageView fitHeight="93.0" fitWidth="67.0" layoutX="147.0" layoutY="139.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../bildes/questionmark.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="97.0" fitWidth="67.0" layoutX="386.0" layoutY="139.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../bildes/flag.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
